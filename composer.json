{
  "name": "ifacesoft/ice",
  "description": "Ice Open Source PHP Framework",
  "homepage": "http://iceframework.net",
  "type": "library",
  "minimum-stability": "stable",
  "keywords": [
    "ifacesoft",
    "ice",
    "iceframework",
    "framework",
    "orm",
    "http",
    "web",
    "database",
    "mysql"
  ],
  "license": "BSD-4-Clause",
  "authors": [
    {
      "name": "dp",
      "email": "denis.a.shestakov@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ice\\": "source/Ice/",
      "Ice\\Core\\": "source/Ice/Core/",
      "Ice\\Helper\\": "source/Ice/Helper/",
      "Ice\\Exception\\": "source/Ice/Exception/",
      "Ice\\DateProvider\\": "source/Ice/DataProvider/",
      "Ice\\Render\\": "source/Ice/Render/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Ifacesoft\\Ice\\Core\\": "test/unit/core",
      "Test\\Ifacesoft\\Ice\\Cli\\": "test/unit/cli",
      "Test\\Ifacesoft\\Ice\\Http\\": "test/unit/http"
    }
  },
  "require": {
    "php": "5.6.* || 7.0.* || 7.1.* || 7.2.* || 7.3.* || 7.4.* || 8.0.*",
    "ifacesoft/ice-cli": "0.0.*",
    "ifacesoft/ice-http": "0.0.*",
    "mrclay/minify": "3.0.*",
    "phpmailer/phpmailer": "6.0.*",
    "phpoffice/phpspreadsheet": "1.17.*.*",
    "tarantool/client": "0.9.*",
    "rybakit/msgpack": "0.8.*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "dev-master",
    "ccampbell/chromephp": "dev-master",
    "lox/xhprof": "dev-master",
    "codeception/codeception": "5.0.x-dev",
  },
  "bin": [
    "bin/ice",
    "bin/ice-exec-ice",
    "bin/ice-release"
  ]
}
